{
  "prod": {
    "app_function": "happy-taps.app",
    "aws_region": "us-east-2",
    "exclude": [
      "__pycache__",
      ".git/*",
      ".gitignore",
      ".python-version",
      "LICENSE",
      "README.md",
      "requirements.txt",
      "zappa_settings.json",
    ],
    "keep_warm": true,
    "keep_warm_expression": "rate(5 minutes)",
    "memory_size": 128,
    "profile_name": "default",
    "project_name": "happy-taps",
    "runtime": "python3.7",
    "s3_bucket": "happy-taps",
    "timeout_seconds": 30,
  }
}
